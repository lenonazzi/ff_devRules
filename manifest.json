{
	"manifest_version": 2,
	"name": "FF Rules",
	"version": "1.0",
	"description": "Show actual window size on resize.",
	"homepage_url": "https://github.com/lenonazzi/ffRules",
	"icons": {
		"48": "data/icons/border-48.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["data/jquery.min.js", "main.js"]
		}
	]
}